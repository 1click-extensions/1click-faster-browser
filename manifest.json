{
  "name": "Web Boost - Faster Web Browsing",
  "short_name": "Webbooost",
  "version": "2.0.0",
  "manifest_version": 2,
  "description": "Make your web pages load faster! Boost browser speed!",
  "homepage_url": "http://www.fse.guru",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/icon16.png",
      "38": "icons/icon32.png"
    },
    "default_title": "Configure WebBoost",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "tabs",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "content_scripts": [],
  "web_accessible_resources": [
    "injectees/*"
  ]
}