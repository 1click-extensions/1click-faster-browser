{
  "name": "WebBooost - Faster Web Browsing",
  "version": "1.6.1",
  "manifest_version": 2,
  "description": "Make your web pages load faster!",
  "homepage_url": "http://www.fse.guru",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_icon": {
      "19": "icons/icon16.png",
      "38": "icons/icon32.png"
    },
    "default_title": "This page was boosted!",
    "default_popup": "page-action.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "content_scripts": [],
  "web_accessible_resources": [
    "injectees/*"
  ]
}